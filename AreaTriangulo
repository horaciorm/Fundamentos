using namespace std;



void main()
{ float altura,base,area=0;
	cout<<"Digite Base: "<<endl;
	cin>>base;

	cout<<"Digite Altura: "<<endl;
	cin>>altura;


	area=(base*altura)/2;
	cout<<"El area es: "<<area;

	getch();
}
