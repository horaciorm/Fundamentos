#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;

int main()
{ int  n,numeros[10];
	int suma=0;

	cout<<"Ingrese el numero de elementos del vector:"<<endl;
	cin>>n;
	for(int i=0;i<n;i++)
	{ cout<<"Ingrese el elemento:["<<i<<"]:";
		cin>>numeros[i];
	suma=suma+numeros[i];}
	cout<<"La suma es:"<<suma<<endl;

	getch();
}

